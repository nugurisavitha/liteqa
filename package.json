{
  "name": "liteqa",
  "version": "1.0.0",
  "description": "AI-assisted test automation platform with Model-Based Testing, AI Test Generation, Self-Healing, Record & Playback, Visual Testing, and Performance Testing",
  "main": "dist/cli/index.js",
  "bin": {
    "liteqa": "./bin/liteqa.js"
  },
  "scripts": {
    "build": "tsc && npm run copy-static",
    "copy-static": "cp -r src/web/public dist/web/",
    "dev": "tsc -w",
    "start": "node dist/cli/index.js web",
    "cli": "node dist/cli/index.js",
    "web": "node dist/cli/index.js web",
    "test": "node dist/cli/index.js run",
    "record": "node dist/cli/index.js record",
    "generate": "node dist/cli/index.js generate"
  },
  "keywords": [
    "test-automation",
    "playwright",
    "appium",
    "api-testing",
    "low-code",
    "ai-testing",
    "model-based-testing",
    "visual-testing",
    "performance-testing",
    "self-healing",
    "record-playback"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "chalk": "^4.1.2",
    "commander": "^11.1.0",
    "express": "^4.18.2",
    "js-yaml": "^4.1.0",
    "multer": "^2.0.2",
    "playwright": "^1.40.0",
    "pngjs": "^7.0.0",
    "socket.io": "^4.7.2",
    "string-similarity": "^4.0.4",
    "webdriverio": "^8.27.0",
    "xlsx": "^0.18.5"
  },
  "devDependencies": {
    "@types/express": "^4.17.21",
    "@types/js-yaml": "^4.0.9",
    "@types/multer": "^2.0.0",
    "@types/node": "^20.10.0",
    "@types/pngjs": "^6.0.4",
    "@types/string-similarity": "^4.0.2",
    "typescript": "^5.3.0"
  }
}
